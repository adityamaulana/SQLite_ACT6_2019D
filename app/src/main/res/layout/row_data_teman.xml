<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmnls:card_view="http://schemas.android.com/apk/res-auto"
    xmlns:xmnls="http://schemas.android.com/tools"

    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:id="@+id/kartuku"

    xmnls:cardElevation="3dp"
    xmnls:cardCornerRadius="5dp"
    xmnls:cardUseCompatPadding="true">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">

        <TextView
            android:id="@+id/textNama"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="TextView"
            android:layout_marginLeft="10dp"/>


    <TextView
        android:id="@+id/textTelpon"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="TextView"
        android:layout_marginLeft="10dp"/>

    </LinearLayout>

</androidx.cardview.widget.CardView>